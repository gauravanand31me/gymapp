{
  "expo": {
    "name": "yupluck",
    "slug": "yupluck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "userInterfaceStyle": "light",
    "scheme": "yupluck",
    "deepLinking": true,
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/exposplash.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF",
      "android": {
        "disable": true
      },
      "ios": {
        "disable": true
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yupluck.mepluck",
      "associatedDomains": [
        "applinks:yupluck.com"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "yupluck"
            ]
          }
        ]
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.yupluck.mepluck",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_foreground.png",
        "backgroundColor": "#0ED94A"
      },
      "permissions": [
        "NOTIFICATION",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "server"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000"
    },
    "extra": {
      "eas": {
        "projectId": "2c1cde6b-706b-416b-9bbe-a415fa7baa74"
      }
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ]
  }
}
